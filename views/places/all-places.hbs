<div class="container">
    <div class="row">
        {{#each allPlaces}}
        <div class="col-md-4">
        <h3>{{name}}</h3>
        <p>{{type}}</p>
        <a href="/places/edit?place_id={{_id}}" class="btn btn-sm btn-dark">Editar</a>
        <a href="/places/delete?place_id={{_id}}" class="btn btn-sm btn-dark">Eliminar</a>
        <hr>
        </div>
        {{/each}}
    </div>
    <div id="placesMap"></div>
    <a href="/" class="btn btn-dark">Go back</a>
</div>

