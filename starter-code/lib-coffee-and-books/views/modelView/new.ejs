
  <body>
		<h3>Add new Places</h3>
		<form method="POST" action="/">
			<label>Name:</label>
			<input type="" name="name" placeholder="Name">
			<label>Description:</label>
			<input type="" name="description" placeholder="Description">

      <select name="placeList">
         <option value="Bookstore">Bookstore</option>
         <option value="CoffeShop">CoffeShop</option>
      </select>

      <label>Address</label>
       <input type="text" name="latitude" placeholder="Latitude" >
       <input type="text" name="longitude" placeholder="Longitude">

			<input type="submit" name="">
		</form>

		<script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
		<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCbBJPvYIaR5ozIx_0-uWiGeO9cK4IxHww"></script>
		<script type="text/javascript" src="/javascripts/new.js"></script>
  </body>
