<!-- main.html -->
<!DOCTYPE html>
<html>
  <head>
    <title>My First Map</title>
  </head>
  <body>
    <h3>Add a coffee place or bookstore</h3>
    <form method="POST" action="/places">
      <p>
      <input type="text" name="name"        placeholder="name">
      </p>
      <p>
      Establishment
      <select name="establishment">
        <option value="coffeeplace">Coffeeplace</option>
        <option value="bookstore">Bookstore</option>
      </select>
      </p>
      <p>
      <label>Address</label>
      <input type="number" name="latitude" placeholder="Latitude" >
      <input type="number" name="longitude" placeholder="Longitude">
      </p>
      <input type="submit" name="">
    </form>
    <br>
    <div id="map"></div>
    <style>
       #map {
        height: 500px;
        width: 100%;
       }
    </style>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.js">
    </script>
    <script  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDUeQXCyJDlhOtCB8JwWAk8zCxpjk6k-jo">
    </script>
    <script type="text/javascript" src="javascripts/main.js"></script>
  </body>
</html>
