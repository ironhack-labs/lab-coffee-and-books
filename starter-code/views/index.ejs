<!DOCTYPE html>
<html>

<head>
  <title>My First Map</title>
  <style>
  #map {
    height: 500px;
    width: 100%;
  }
  </style>
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
</head>

<body>
  <h3>My First Map</h3>
  <div id="map"></div>
  <div>
    <ul>
      <%places.forEach(place => {%>
        <li>
          <%=place.name%> : Lat :
            <%=place.location.coordinates[0]%>
              Long :
              <%=place.location.coordinates[1]%>
        </li>
    </ul>
    <% }) %>
  </div>
  <form class="" action="/" method="post">
    <input type="text" name="lat" placeholder="Ecris la latitude">
    <input type="text" name="lng" placeholder="Ecris la longitude">
    <input type="text" name="name" placeholder="Ecris la longitude">
    <input type="text" name="description" placeholder="description">
    <button type="submit" id="submit" name="button">Submit</button>
  </form>
  <script type="text/javascript" src="javascripts/main.js"></script>
  <script type="text/javascript">
  var myPlaces = <%- JSON.stringify(places) %>;
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBDEl0Nn4CYN1n408gz66TRMMcHyiabJ9g"></script>
</body>

</html>