
  <div class="placesList">
    {{#each places}}
    <div class="listItem">
      <li>Name of place: {{name}}</li>
      <li>Type of place: {{type}}</li>
      <a class="edit" href="/{{_id}}/edit">Edit</a>
      <a class="delete" href="/{{_id}}/delete">Delete</a>
    </div>
    {{/each}}
  </div>

<script src="/javascripts/index.js"></script>

