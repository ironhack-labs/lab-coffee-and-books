<p> <a href="/new">New market</a></p>


{{#if allPlaces  }}
<ul>
    {{#each allPlaces}}
    <li>
        <p>{{this.name}}</p>
        <a href="/{{this._id}}/edit-place">Edit</a>
        <form action="/{{this._id}}/delete" method="POST">
            <button type="submit">Delete</button>
        </form>
    </li>
    {{/each}}
</ul>
{{/if}}


<div id="map"></div>


<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key={{process.env.API_KEY}}"></script>
<script type="text/javascript" src="/javascripts/main.js"></script>