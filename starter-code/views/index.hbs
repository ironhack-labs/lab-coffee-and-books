<h1>{{title}}</h1>
<div class="info">
    <h3>List of all coffee shops</h3>
    <a href="/places/new">Create new coffee shop</a>
    <ul>
        {{#each places}}
        <li>
            <a href="/places/{{ this._id}}">{{ this.name}}</a>
            |
            <a href="/places/{{ this._id}}/delete">Delete</a>
        </li>
        {{/each }}
    </ul>
    <figure id="map"></figure>
</div>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDG88rK2sVwRDbZIJautuIZpNt32kAQpSU"></script>
<script src="/javascripts/main.js"></script>


