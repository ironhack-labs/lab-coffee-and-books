
<a href="/new">Add a new marker</a>

<ul>
  {{#each allPlaces}}
  <li>
    <p>{{this.name}}</p>
    <a href="/{{this._id}}/edit-place">edit</a>
    <form action="/{{this._id}}/delete" method="POST">
      <button type="submit">delete</button>
    </form>
  </li>
  {{/each}}
</ul>


<div id="map"></div>



