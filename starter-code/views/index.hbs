<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>


<div id="map"></div>

<form method="POST" action="/places">
  <input type="text" name="name" placeholder="name">
  <input type="text" name="description" placeholder="description">

  <label>Address</label>
  <input type="text" name="latitude" placeholder="latitude" id="latitude" >
  <input type="text" name="longitude" placeholder="longitude" id="longitude">

  <button type="submit" name="">CREATE OR EDIT</button>
</form>

<form method="POST" action="/places/delete">
  <input type="text" name="name" placeholder="insert place's name to be deleted">

  <button type="submit" name="">DELETE</button>
</form>

<script>
  window.result = {{{result}}}
</script>
