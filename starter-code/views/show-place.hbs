<h3 id="placeName" data-id="{{ place.id}}">{{ place.name}}</h3>
<p>{{ place.type}}</p>
<p>Latitude {{place.location.coordinates.[1]}}</p>
<p>Longitude {{place.location.coordinates.[0]}}</p>

<figure id="mymap" ></figure>

<a href="/places/{{place._id}}/edit">Edit this place</a>
<a href="/places">Go back to the list</a>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBOHtSnhKgb6hKkIy2uu6ZmvdlRZEwW8dA"></script>
<script src="/javascripts/main.js"></script>
