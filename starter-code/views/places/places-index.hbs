<div class="container places-index">

    <a href="/places/new" class="btn btn-dark">Crear un nuevo place</a>

    <h1 style="font-weight: 100">Índice de Place Index</h1>


    {{#each allPlace}}
    <a href="/places/{{_id}}" class="btn btn-sm btn-dark">Ver detalles</a>
    <h4>Nombre: {{name}}</h4>
    <p>{{type}}</p>
    <strong>Dirección: </strong> {{location}} |
    <hr>
    {{/each}}



</div>