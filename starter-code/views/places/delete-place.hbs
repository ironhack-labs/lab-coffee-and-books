<h2>Which place would you like to delete?</h2>
<form action="/delete-place" method="POST">
    <select name="place_id">
        <option value="" disabled>select by name</option>
        {{#each allPlaces}}
        <option value="{{this.id}}">{{this.name}} ({{this.type}})</option>
        {{/each}}
    </select>
    <input type="submit" value="Delete">
</form>