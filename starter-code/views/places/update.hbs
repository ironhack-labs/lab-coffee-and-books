<h3>Edita coffe/books</h3>
<form method="POST" action="/places/{{ places._id}}">
    <label>Name:</label>
    <input type="" name="name" placeholder="Name" value="{{ places.name}}">
    {{!-- <label>Description:</label>
    <input type="" name="description" placeholder="Description" value="{{ places.description}}"> --}}
    <label>Address</label>
    <input type="text" name="latitude" id="latitude" placeholder="Latitude"
        value="{{ places.location.coordinates.[1]}}">
    <input type="text" name="longitude" id="longitude" placeholder="Longitude"
        value="{{ places.location.coordinates.[0]}}">
    <button type="submit" name="" id="submit">Edit</button>
</form>