<h3>Edit a place</h3>
<form method="POST" action="/places/{{ place._id }}">
  <label>Place name:</label>
  <input type="" name="name" placeholder="Nombre" value="{{ place.name}}">
  <label>Type:</label>
  <input type="" name="type" placeholder="Tipo de place" value="{{ place.type}}">
  <label>Address</label>
  <input type="text" name="latitude" id="latitude" placeholder="Latitud" value="{{ place.location.coordinates.[1]}}">
  <input type="text" name="longitude" id="longitude" placeholder="Longitud" value="{{ place.location.coordinates.[0]}}">
  <button type="submit" name="" id="submit">Edit</button>
  <a href="/">back to index</a>
  <a href="/places">Place list</a>
</form>