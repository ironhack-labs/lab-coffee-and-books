<h1>New Place</h1>

<form action="/places/new" method="post">
  <label for="name">Name:</label>
  <input type="text" name="name" id="name">
  <label for="type">Type:</label>
  <select name="type" id="type">
    <option value="COFFESHOP">Coffee Shop</option>
    <option value="BOOKSTORE">Bookstore</option>
  </select>


    <label>Address</label>
  <input type="text" name="latitude" placeholder="latitude" id="latitude" >
  <input type="text" name="longitude" placeholder="longitude" id="longitude">

  
  <button type="submit">Create Place</button>
</form>

<a href="/places">Go Back</a> 

  <div id="map"></div>
