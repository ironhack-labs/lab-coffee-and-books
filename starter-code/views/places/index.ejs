<!DOCTYPE html>
<html>
  <head>
    <title>The Best Places on Earth</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="container">
      </div>
      <div class="info">
        <h3>List all places</h3>
        <a href="/new">Create new place</a>
        <ul>
          <% places.forEach((place) => { %>
            <li>
              <a href="/<%= place._id%>">
                <%= place.name%><span><%= place.description %></span>
              </a>
               |
              <a href="/<%= place._id%>/edit">Edit</a>
               |
              <a href="/<%= place._id%>/delete">Delete</a>
            </li>
          <% }) %>
        </ul>
        <div id="map"></div>
      </div>
    </div>
  </body>
    <script type="text/javascript">
     var myPlaces = (<%- jsonPlaces %>);
   </script>
  <script  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDZXQMP2GuykxkAKb5m3NpmBJ1NvIZ0WfA">
  </script>
  <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
  <script type="text/javascript" src="/javascripts/main.js"></script>
</html>
