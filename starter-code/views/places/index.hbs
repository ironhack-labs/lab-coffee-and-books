<div class="info">
    <h3>List of all places</h3>
    <a href="/new">Create new place</a>
    <ul>
        {{#each place}}
        <li>
            <a href="/places/{{this._id}}">{{this.name}}</a>
            |
            <a href="/places/{{this._id}}/delete">Delete</a>
        </li>
        {{/each }}
    </ul>
    <figure id="map"></figure>
</div>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBPAXUe68K_Kwb6YdAIfh5MO48LI21tWX8"></script>
<script type="text/javascript" src="/javascripts/main.js"></script>