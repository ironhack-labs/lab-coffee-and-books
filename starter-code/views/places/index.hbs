<h1>{{title}}</h1>
<div class="info">
    <h3>List of all Places</h3>
    <a href="/places/new">Create new Place</a>
    <ul>
        {{#each places}}
        <li>
            <a href="/places/{{ this._id}}">{{ this.name}}</a>
            |
            <a href="/places/{{ this._id}}/delete">Delete</a>
        </li>
        {{/each }}
    </ul>
    <div id="map"></div>
</div>
