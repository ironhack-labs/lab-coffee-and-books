<head>
  <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
  <script type="text/javascript" src="/javascripts/map.js"></script>
  <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>
  <form action="/places/new" method="POST">
    <label for="name">Name</label>
    <input type="text" name="name" placeholder="bookShop">

    <label for="description">Description</label>
    <input type="text" name="description" placeholder="description">

    <label for="localType">Local Type</label>
    <select name="localType">
      <option value="Coffee">Coffee</option>
      <option value="Book">Book</option>
    </select>

    <label>Address</label>
    <input type="text" name="address" id='address'>
    <button type="button" id='submit'>Get Values</button>
    <label>Lat & Long</label>
    <input type="text" name="lat" placeholder="latitude" id="lat">
    <input type="text" name="long" placeholder="longitude" id='long'>

    <button>Submit</button>
  </form>

  <div id="map"></div>

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDndv3ZEEkcMesyPu_FuA_RBm-aFj-B1XE "></script>
</body>
