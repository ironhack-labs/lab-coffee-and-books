<form action="/places/edit/{{place._id}}" method="POST">
  <label for="">Name:</label>
  <input type="text" name="name" value="{{place.name}}">

  <label for="">Type:</label>
  <select name="type" value="{{place.type}}">
      <option value="{{place.type}}" selected>{{place.type}}</option>
      <option value="{{otherTypes.type}}">{{otherTypes.type}}</option>
  </select>

  <label>Location:</label>
  <input type="text" name="latitude" value="{{place.location.coordinates.[1]}}" >
  <input type="text" name="longitude" value="{{place.location.coordinates.[0]}}">

  <button type="submit">UPDATE</button>
</form>

<div id="map"></div>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key={{MAPSKEY}}"></script>
<script src="/javascripts/script.js"></script>