<div class="info">
    <h3>Place: {{this.name}}</h3>
  <form action="/edit/edit?celebrity_id={{this.id}}" method="POST">
  <label for="name">Name:</label>
  <input type="text" name="name" value="{{this.name}}"><br>
  <label for="type">Type:</label>
  <input type="text" name="type" value="{{this.type}}"><br>
  <label for="latitude">Latitude:</label>
  <input type="text" name="latitude" value="{{this.latitude}}"><br>
  <label for="longitude">Longitude:</label>
  <input type="text" name="longitude" value="{{this.longitude}}"><br>
  <button type="submit">Actualizar</button>
</form>
<a href="/">Cancelar y volver a la lista</a>
  
</div>