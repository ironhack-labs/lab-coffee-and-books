<form action="/places/edit/{{place._id}}" method="post">
  <label for="">Name:</label>
  <input type="text" name="name" value="{{place.name}}">

  <label for="">Type:</label>
 <select name="type">
    <option value="coffee shop">Coffee shop</option>
    <option value="bookstore">Bookstore</option>
  </select>

    <label>Location</label>
  <input type="text" name="latitude"  value="{{#if place.location}}{{place.location.coordinates.[1]}}{{/if}}" >
  <input type="text" name="longitude" value="{{#if place.location}}{{place.location.coordinates.[0]}}{{/if}}">

  <button type="submit">EDIT</button>
</form>