<form action='/create' method='POST'>
    <div>
        <label for = 'name'>Comercial Name:</label>
        <input type='text' name='name' required/> 
    </div>
    <div>
    <label for = 'type'>Comercial Type:</label>
    <select name='type' value="" >
      <option value="Coffe Shop">Coffe Shop</option>
      <option value="Book Store">Book Store</option>
    </select>
    </div>
    <label>Address:</label>
    <input type='text' name='lat' placeholder="Latitud" required/> 
    <input type='text' name='lng' placeholder="Langitud" required/> 
    <div>
    <input type="submit"></input>
    </div>
</form>