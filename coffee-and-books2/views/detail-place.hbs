<h2>Place Details</h2>


<div>
    <label>
        Name :
        <span>{{name}}</span>
    </label>
</div>

<div>
    <label>
        Type of Place :
        <span>{{type}}</span>
    </label>
</div>

<div>
    <label>
        Latitud:
        <span>{{location.coordinates.[0]}}</span>
    </label>
</div>

<div>
    <label>
        Longitud
        <span>{{location.coordinates.[1]}}</span>
    </label>
</div>

<div>
    <a href="/places/{{_id}}/edit">Edit Place</a>
</div>
